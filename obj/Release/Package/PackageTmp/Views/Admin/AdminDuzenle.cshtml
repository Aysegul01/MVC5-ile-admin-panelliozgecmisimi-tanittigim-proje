@model MvcCv.Models.Entity.TblLogin 

@{
    ViewBag.Title = "AdminDuzenle";
    Layout = "~/Views/Shared/AdminPaneliLayout.cshtml";
}



<br />
<h4>  Şifrenizi Güncelleyiniz.</h4>
<br />
<form method="post">
    @Html.Label("KullaniciAdi")
    @Html.TextBoxFor(x => x.KullaniciAdi, new { @class = "form-control" })
