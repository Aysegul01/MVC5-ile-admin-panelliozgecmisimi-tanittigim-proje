@using MvcCv.Models.Entity;
@model List<TblEducation>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminPaneliLayout.cshtml";
}


<h2>EĞİTİMLERİM</h2>

<table class=" table table-bordered">
    <tr>
        <th>ID</th>
        <th>Başlık</th>
        <th>Alt Başlık</th>
        <th>Alt Başlık 2</th>
        <th>GNO</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.ID</td>
    <td>@item.Baslik</td>
    <td>@item.Altbaslik1</td>
    <td>@item.Altbaslik2</td>
    <td>@item.GNO</td>
    <td>@item.Tarih</td>
    <td><a href="/Egitim/EgitimSil/@item.ID" class="btn btn-danger">Sil</a></td>
    <td><a href="/Egitim/EgitimDuzenle/@item.ID" class="btn btn-success">Düzenle</a></td>
</tr>
    }
</table>
<a href="/Egitim/EgitimEkle" class="btn btn-primary">Yeni Eğitim Bilgisi Ekle</a>
