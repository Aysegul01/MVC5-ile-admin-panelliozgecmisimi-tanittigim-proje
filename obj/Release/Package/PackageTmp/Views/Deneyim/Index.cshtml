@using MvcCv.Models.Entity;
@model List<TblExperıence>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminPaneliLayout.cshtml";
}

<h2>Deneyim</h2>

<table class=" table table-bordered">
    <tr>
        <th>ID</th>
        <th>Başlık</th>
        <th>Alt Başlık</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach(var item in Model) {
<tr>
    <td>@item.ID</td>
    <td>@item.Baslik</td>
    <td>@item.Altbaslik</td>
    <td>@item.Tarih</td>
    <td><a href="/Deneyim/DeneyimSil/@item.ID" class="btn btn-danger">Sil</a></td>
    <td><a href="/Deneyim/DeneyimGetir/@item.ID" class="btn btn-success">Düzenle</a></td>
</tr>
    }
</table>
<a href="/Deneyim/DeneyimEkle" class="btn btn-primary">Yeni Deneyim Ekle</a>